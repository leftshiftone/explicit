<accommodation>
    <rule>
        <eql>simple string extractor</eql>
        <ner>
            <one>"A"</one>
            <two>"B"</two>
        </ner>
    </rule>
    <rule>
        <eql>simple number extractor</eql>
        <ner>
            <one>1</one>
            <two>2</two>
        </ner>
    </rule>
    <rule>
        <eql>simple boolean extractor</eql>
        <ner>
            <one>true</one>
            <two>false</two>
        </ner>
    </rule>
    <rule>
        <eql>simple variable:x extractor:y</eql>
        <ner>
            <one>$x</one>
            <two>$y</two>
        </ner>
    </rule>
    <rule>
        <eql>simple function:x extractor:y</eql>
        <ner>
            <one>uppercase($x)</one>
            <two>uppercase($y)</two>
        </ner>
    </rule>
</accommodation>
